{
  "name": "gerbtrace",
  "version": "1.3.0",
  "packageManager": "pnpm@10.30.3+sha512.c961d1e0a2d8e354ecaa5166b822516668b7f44cb5bd95122d590dd81922f606f5473b6d23ec4a5be05e7fcd18e8488d47d978bbe981872f1145d06e9a740017",
  "private": true,
  "description": "Gerbtrace â€” View and compare Gerber PCB files in the browser",
  "author": "Newmatik GmbH",
  "license": "BUSL-1.1",
  "type": "module",
  "engines": {
    "node": ">=22.20.0 <23"
  },
  "scripts": {
    "libraries:sync": "node scripts/sync-cad-libraries.mjs",
    "libraries:parse": "node scripts/parse-cad-libraries-to-tpsys.mjs",
    "libraries:parse:tht": "node scripts/parse-cad-libraries-to-tht.mjs",
    "packages:migrate:newmatik": "node scripts/migrate-builtins-to-library-tree.mjs",
    "packages:cleanup:legacy-root": "node scripts/cleanup-legacy-package-root.mjs",
    "packages:manifest": "node scripts/build-packages-manifest.mjs",
    "packages:tht:manifest": "node scripts/build-tht-packages-manifest.mjs",
    "packages:check": "node scripts/check-packages.mjs",
    "packages:tree:manifest": "node scripts/build-packages-manifest.mjs",
    "packages:tree:check": "node scripts/check-packages.mjs",
    "packages:extract-mycenter": "node scripts/extract-mycenter-packages.mjs",
    "typecheck": "pnpm exec nuxi typecheck",
    "precommit": "pnpm run packages:manifest && pnpm run packages:check && (git add -u public/packages/libraries/_tree.json public/packages/_manifest.json 2>/dev/null || true) && pnpm run typecheck",
    "dev": "pnpm run precommit && cross-env CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=400 nuxt dev --logLevel verbose --no-fork",
    "dev:debug": "pnpm run precommit && cross-env CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=400 nuxt dev --logLevel verbose --no-fork",
    "build": "nuxt build",
    "deploy": "nuxt build && wrangler deploy",
    "generate": "nuxt generate && node scripts/prune-static-output.mjs",
    "tauri:prepare-frontend": "node scripts/prepare-tauri-frontend.mjs",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && simple-git-hooks",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "node scripts/tauri-build.mjs",
    "supabase:start": "pnpm exec supabase start",
    "supabase:stop": "pnpm exec supabase stop",
    "supabase:migrate": "pnpm exec supabase db push",
    "supabase:reset": "pnpm exec supabase db reset",
    "supabase:seed": "pnpm exec supabase db reset --seed-only",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@nuxt/content": "^3.11.2",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/seo": "^3.4.0",
    "@sentry/nuxt": "^10.39.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/vue-virtual": "^3.13.18",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.10.0",
    "@tauri-apps/plugin-window-state": "^2.4.1",
    "better-sqlite3": "^12.6.2",
    "dexie": "^4.3.0",
    "diff": "^8.0.3",
    "jszip": "^3.10.1",
    "nuxt": "^4.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.90",
    "@tauri-apps/cli": "^2.10.0",
    "@types/diff": "^7.0.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.3.1",
    "playwright": "^1.58.2",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5",
    "simple-git-hooks": "^2.13.1",
    "supabase": "^2.76.8",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.68.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run precommit"
  },
  "overrides": {
    "@vercel/nft": "^0.27.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "workerd"
    ]
  }
}
